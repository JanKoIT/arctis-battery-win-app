{
  "name": "steel-series-battery",
  "productName": "steel-series-battery",
  "version": "0.2.2",
  "description": "Shows SteelSeries Arctis wireless device's battery percentages as a tray icon.",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run compile && electron-forge start",
    "compile": "sass src/styles/styles.scss > src/styles.css",
